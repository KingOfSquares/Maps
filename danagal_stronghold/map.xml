    <negative id="tier-2-chests">
        <union>
            <region id="tier-1-chests"/>
            <region id="tier-3-chests"/>
        </union>
    </negative>
    <!-- Tier 3 Chests (Very middle) -->
    <union id="tier-3-chests">
        <sphere radius="10" origin="-287.5,74.5,-94.5"/>
    </union>
</regions>
<objective>Free for all, be the last man standing!</objective>
<broadcasts>
    <alert after="3s">Chests refill after 3 and 5 minutes!</alert>
    <alert after="3m">All chests have been refilled!</alert>
    <alert after="5m">All chests have been refilled!</alert>
</broadcasts>
<players min="4" max="8" max-overfill="8"/>
<time result="objectives">8m</time>
<score>
    <kills>1</kills>
</score>
<blitz>
    <lives>1</lives>
    <broadcastLives>false</broadcastLives>
</blitz>
<gamerules>
    <doFireTick>true</doFireTick>
    <doDaylightCycle>false</doDaylightCycle>
</gamerules>
<tnt>
    <instantignite>on</instantignite>
    <fuse>2s</fuse>
    <licensing>false</licensing>
</tnt>
<mobs>
    <filter>
        <spawn>spawner egg</spawn>
    </filter>
</mobs>
<kits>
    <kit id="spawn-effects">
        <potion duration="8" amplifier="5">resistance</potion>
        <potion duration="8" amplifier="10">saturation</potion>
    </kit>
</kits>
<filters>
    <any id="chest-refill">
        <all>
            <time>3m</time>
            <not>
                <time>3m1s</time>
            </not>
        </all>
        <all>
            <time>5m</time>
            <not>
                <time>5m1s</time>
            </not>
        </all>
    </any>
    <all id="tier-1-only-chests-after-5m">
        <all id="tier-1-only-chests">
            <region id="tier-1-chests"/>
            <material>chest</material>
        </all>
        <time>4m59s</time>
    </all>
    <all id="tier-2-only-chests-after-5m">
        <all id="tier-2-only-chests">
            <region id="tier-2-chests"/>
            <material>chest</material>
        </all>
        <time>4m59s</time>
    </all>
    <all id="tier-3-only-chests">
        <region id="tier-3-chests"/>
        <material>chest</material>
    </all>
    <not id="not-chest">
        <material id="only-chest">chest</material>
    </not>
</filters>
<regions>
