<?xml version="1.0"?>
<map proto="1.4.3">
<name>Portals</name>
<version>3.0.37</version>
<objective>Leak lava from the enemy's obsidian core into the void.</objective>
<authors>
    <author uuid="177803b2-797c-4089-aae1-5702ca259e2c" /> <!-- Onsole -->
</authors>
<teams>
    <team color="blue" max="100" id="blue-team">Blue</team>
    <team color="dark red" max="100" id="red-team">Red</team>
</teams>
<rage />
<time>8m</time>
<score>
    <kills>1</kills>
    <deaths>-1</deaths>
</score>
<kits>
    <!-- Kit and Filter References -->
    <lend kit="jumper-kit" filter="carrying-gold" />
    <give kit="regen-kit" filter="crouching" />
    <!-- Inline kit and filters -->
    <take>
        <kit>
            <fly/>
        </kit>
        <filter>
            <flag-carried>flag</flag-carried>
        </filter>
    </take>
    <kit id="jumper-kit">
        <fly/>
        <max-health>10</max-health>
        <effect duration="oo" amplifier="4">speed</effect>
    </kit>
    <kit id="regen-kit">
        <effect duration="4" amplifier="4">regeneration</effect>
    </kit>
    <kit id='default-kit' parent="chestplate">
        <item amount='64'>stone</item>
        <item material="diamond" />
        <item material="redstone" />
        <item material="diamond chestplate" damage="527"/>
    </kit>
    <kit id="extra-items">
        <item material="iron ingot" />
        <item material="gold ingot" />
    </kit>
    <kit id="chestplate">
        <item material="gold ingot" />
        <!-- <chestplate material="diamond chestplate" /> -->
    </kit>
</kits>
<spawns>
    <spawn yaw="-90" team="blue-team" kit="default-kit">
        <region>
            <point>17,15,-65</point>
        </region>
    </spawn>
    <spawn yaw="-90" team="red-team" kit="default-kit">
        <region>
            <point>17,15,-65</point>
        </region>
    </spawn>
    <default yaw="-90">
        <region>
            <point>20,20,-65</point>
        </region>
    </default>
</spawns>
<flags>
    <flag id="flag">
        <post>11.5,15,-52.5</post>
    </flag>
</flags>
<filters>
    <all id="combo-filter">
        <crouching id="crouching" />
        <holding>
            <item material="diamond" />
        </holding>
    </all>
    <holding id="holding-redstone">
        <item material="redstone" />
    </holding>
    <holding id="holding-iron">
        <item material="iron ingot" />
    </holding>
    <holding id="holding-gold">
        <item material="gold ingot" />
    </holding>

    <carrying id="carrying-gold"><item material="gold ingot" /></carrying>


    
    
    
    <not id="not-wearing-chestplate"><wearing id="wearing-chestplate"><item material="diamond chestplate"/></wearing></not>

    <flag-carried id="flag-carry">flag</flag-carried>

    <objective id="objective-event">objective</objective>

    <score id="score-range-filter">[2, 6)</score>

    <lives id="lives-filter" min="3" max="5"/>

</filters>
